**DRAFT**


def binary_search(list1, num):
    left = 0
    right = len(list1) - 1
    while left <= right:
        mid = (left + right) // 2
        if list1[mid] == num:
            return mid
        elif list1[mid] < num:
            left = mid + 1
        else:
            right = mid - 1

def count_frequency(list1, num):
    index = binary_search(list1, num)

    if index == -1:
        return 0

    count = 1

    # Count occurrences to the left of the found index
    left_index = index - 1
    while left_index >= 0 and list1[left_index] == num:
        count += 1
        left_index -= 1

    # Count occurrences to the right of the found index
    right_index = index + 1
    while right_index < len(list1) and list1[right_index] == num:
        count += 1
        right_index += 1

    return count
